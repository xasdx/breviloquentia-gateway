{
  "name": "breviloquentia-gateway",
  "version": "1.0.0",
  "dependencies": {
    "@nestjs/common": "^4.5.6",
    "@nestjs/core": "^4.5.7",
    "@nestjs/microservices": "^4.5.7",
    "@nestjs/testing": "^4.5.5",
    "@nestjs/typeorm": "^2.0.0",
    "@nestjs/websockets": "^4.5.7",
    "@types/node": "^9.3.0",
    "jest": "^22.0.6",
    "jest-to-match-shape-of": "^1.0.0",
    "mysql": "^2.15.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.6",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.1",
    "ts-node": "^4.1.0",
    "typeorm": "^0.1.11",
    "typescript": "^2.6.2"
  },
  "scripts": {
    "start": "node index.js",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "test": "jest --forceExit"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "^((?!c9).)*test/.+\\.spec\\.ts$",
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": ["/test"]
  }
